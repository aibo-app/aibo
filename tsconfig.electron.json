  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "./dist-electron",
    "rootDir": "./electron",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "incremental": true,
    "tsBuildInfoFile": "./dist-electron/.tsbuildinfo"
  },
  "include": [
    "electron/**/*"
  ]
}